<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.treasure.hunt.view.widget.SaveAndLoadController">
   <children>
      <Button mnemonicParsing="false" onAction="#save" text="Save to file"/>
      <Button mnemonicParsing="false" onAction="#load" text="Load"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
</HBox>
